{
    "name": "enterprise-flow",
    "version": "1.0.0",
    "description": "Enterprise-Grade API Orchestration Platform",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "ts-node-dev --respawn src/index.ts",
      "test": "jest",
      "test:unit": "jest --testPathPattern=tests/unit",
      "test:integration": "jest --testPathPattern=tests/integration",
      "test:performance": "jest --testPathPattern=tests/performance",
      "lint": "eslint --ext .ts src/",
      "format": "prettier --write \"src/**/*.ts\"",
      "docker:build": "docker build -t enterprise-flow .",
      "docker:run": "docker run -p 3000:3000 enterprise-flow"
    },
    "keywords": [
      "api",
      "gateway",
      "microservices",
      "orchestration",
      "enterprise"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
      "axios": "^1.6.2",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-rate-limit": "^7.1.5",
      "helmet": "^7.1.0",
      "ioredis": "^5.3.2",
      "jsonwebtoken": "^9.0.2",
      "mongodb": "^6.3.0",
      "opentelemetry-node": "^0.35.1",
      "pino": "^8.16.2",
      "prometheus-client": "^0.5.0",
      "uuid": "^9.0.1",
      "winston": "^3.11.0"
    },
    "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.10",
      "@types/node": "^20.10.4",
      "@types/uuid": "^9.0.7",
      "@typescript-eslint/eslint-plugin": "^6.13.2",
      "@typescript-eslint/parser": "^6.13.2",
      "eslint": "^8.55.0",
      "jest": "^29.7.0",
      "prettier": "^3.1.0",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.3.2"
    },
    "engines": {
      "node": ">=16.0.0"
    }
  }